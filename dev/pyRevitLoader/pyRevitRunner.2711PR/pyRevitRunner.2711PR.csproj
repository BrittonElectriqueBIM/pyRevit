<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>pyRevitRunner</AssemblyName>
    <RootNamespace>pyRevitRunner</RootNamespace>
    
    <DefineConstants>$(DefineConstants);PYREVITLABS_ENGINE</DefineConstants>
    <UseRunner>true</UseRunner>
    <IronPythonVersion>IPY2711PR</IronPythonVersion>
    <UsePyRevitIronPython>true</UsePyRevitIronPython>
    <IronPythonStdLib>python_2711pr_lib.zip</IronPythonStdLib>
    <Version>2.7.11</Version>
  </PropertyGroup>
</Project>